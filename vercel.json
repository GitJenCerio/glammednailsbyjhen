{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/google/sync",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/release-expired",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/cleanup-slots",
      "schedule": "0 0 * * *"
    }
  ]
}

